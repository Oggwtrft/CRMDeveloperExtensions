<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>CRM Solution Package</Name>
    <Description>Dynamics CRM Solution Package</Description>
    <DefaultName>CRMSolutionPackage</DefaultName>
    
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType></ProjectSubType>
    <SortOrder>1000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>    
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <Icon>sw-file-icon.png</Icon>
    <TemplateID>542994ee-2321-45f4-a52a-da50c20f0e6a</TemplateID>
    <!-- Indicates how many parent folders this item template should appear in -->
    <NumberOfParentCategoriesToRollUp>1</NumberOfParentCategoriesToRollUp>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="CRMSolutionPackage.csproj" File="CRMSolutionPackage.csproj" ReplaceParameters="true">      
    </Project>
  </TemplateContent>
  <WizardExtension>
    <Assembly>TemplateWizards, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null</Assembly>
    <FullClassName>TemplateWizards.VersionWizard</FullClassName>
  </WizardExtension>
  <WizardData>
    <!--Valid values are Plug-in or Workflow or Item-->
    <CRMProjectType>Package</CRMProjectType>
    <NeedsClient>False</NeedsClient>
    <IsUnitTest>False</IsUnitTest>
    <IsUnitTestItem>False</IsUnitTestItem>
  </WizardData>
</VSTemplate>