<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Get Is Auto Save Enabled (7.0+)</Title>
      <Shortcut>XrmContextGetIsAutoSaveEnabled</Shortcut>
      <Description>7.0+ Returns whether Autosave is enabled for the organization.</Description>
      <HelpUrl>https://msdn.microsoft.com/en-us/library/gg334511.aspx#BKMK_getIsAutoSaveEnabled</HelpUrl>
      <Author>Jason Lattimer</Author>
      <Keywords>
        <Keyword>Dynamics CRM</Keyword>
        <Keyword>CRM</Keyword>
        <Keyword>Context</Keyword>
        <Keyword>2015</Keyword>
        <Keyword>2016</Keyword>
      </Keywords>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Code Language="JavaScript">
        <![CDATA[Xrm.Page.context.getIsAutoSaveEnabled()$end$]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
