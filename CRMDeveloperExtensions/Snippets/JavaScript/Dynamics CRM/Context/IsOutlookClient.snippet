<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Is Outlook Client (5.0-6.1)</Title>
      <Shortcut>XrmContextIsOutlookClient</Shortcut>
      <Description>5.0-6.1 Returns a Boolean value indicating if the user is using CRM for Outlook.</Description>
      <HelpUrl>https://msdn.microsoft.com/en-us/library/gg334511(v=crm.6).aspx#BKMK_isoutlookclient</HelpUrl>
      <Author>Jason Lattimer</Author>
      <Keywords>
        <Keyword>Dynamics CRM</Keyword>
        <Keyword>CRM</Keyword>
        <Keyword>Context</Keyword>
        <Keyword>2011</Keyword>
        <Keyword>2013</Keyword>
      </Keywords>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Code Language="JavaScript">
        <![CDATA[Xrm.Page.context.isOutlookClient()$end$]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
