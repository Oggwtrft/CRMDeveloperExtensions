<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Set Tab Visible (5.0+)</Title>
      <Shortcut>XrmTabsSetVisible</Shortcut>
      <Description>5.0+ Sets a value that indicates whether the control is visible.</Description>
      <HelpUrl>https://msdn.microsoft.com/en-us/library/gg328067.aspx#BKMK_tabsetVisible</HelpUrl>
      <Author>Jason Lattimer</Author>
      <Keywords>
        <Keyword>Dynamics CRM</Keyword>
        <Keyword>CRM</Keyword>
        <Keyword>Tabs</Keyword>
        <Keyword>2011</Keyword>
        <Keyword>2013</Keyword>
        <Keyword>2015</Keyword>
        <Keyword>2016</Keyword>
      </Keywords>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Code Language="JavaScript">
        <![CDATA[Xrm.Page.ui.tabs.get(null|"tabname"|tabindex).setVisible(true|false)$end$]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>

