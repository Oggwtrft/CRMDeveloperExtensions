<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Get Form Selectors (5.0+)</Title>
      <Shortcut>XrmFormSelectorItems</Shortcut>
      <Description>5.0+ A collection of all the forms on the page.</Description>
      <HelpUrl>https://msdn.microsoft.com/en-us/library/gg327828.aspx#BKMK_controls</HelpUrl>
      <Author>Jason Lattimer</Author>
      <Keywords>
        <Keyword>Dynamics CRM</Keyword>
        <Keyword>CRM</Keyword>
        <Keyword>FormSelector</Keyword>
        <Keyword>2011</Keyword>
        <Keyword>2013</Keyword>
        <Keyword>2015</Keyword>
        <Keyword>2016</Keyword>
      </Keywords>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Code Language="JavaScript">
        <![CDATA[Xrm.Page.ui.formSelector.items.get()$end$]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
