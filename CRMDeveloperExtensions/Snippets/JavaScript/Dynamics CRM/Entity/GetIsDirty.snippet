<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Get IsDirty (5.0+)</Title>
      <Shortcut>XrmEntityGetIsDirty</Shortcut>
      <Description>5.0+ Returns a Boolean value that indicates if any fields in the form have been modified.</Description>
      <HelpUrl>https://msdn.microsoft.com/en-us/library/gg334720.aspx#BKMK_getIsDirty</HelpUrl>
      <Author>Jason Lattimer</Author>
      <Keywords>
        <Keyword>Dynamics CRM</Keyword>
        <Keyword>CRM</Keyword>
        <Keyword>Entity</Keyword>
        <Keyword>2011</Keyword>
        <Keyword>2013</Keyword>
        <Keyword>2015</Keyword>
        <Keyword>2016</Keyword>
      </Keywords>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Code Language="JavaScript">
        <![CDATA[Xrm.Page.data.entity.getIsDirty()$end$]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>
