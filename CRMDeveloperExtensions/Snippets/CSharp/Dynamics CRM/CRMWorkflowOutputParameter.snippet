      <Author>Jason Lattimer</Author>
      <Keywords>
        <Keyword>Dynamics CRM</Keyword>
        <Keyword>CRM</Keyword>
        <Keyword>Output</Keyword>
        <Keyword>Output Parameter</Keyword>
        <Keyword>Workflow</Keyword>
      </Keywords>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Code Language="CSharp">
        <![CDATA[[Output("CRM Display Text")]
        public OutArgument<DataType> OutputParameterName { get; set; }$end$]]>
      </Code>
    </Snippet>
